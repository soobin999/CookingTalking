<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cook.talk.model.dao.TalkDAO">
<select id="listAll" resultType="com.cook.talk.model.VO.TalkVO">
select * from talk ORDER BY talkCode desc,talkDate desc
</select>

<!-- 게시글 작성 -->
<insert id="wirte">
insert into talk (talkCode,talkCont, wirter,talkVideo) values
(#{talkCode},#{talkCont},#{wirter},#{talkVideo})
</insert>

















</mapper>